<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="j012.tobalobsecommerce">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/config_network"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TobaLobsEcommerce"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activity.DetailTransk"></activity>
        <activity android:name=".activity.Transfer" />
        <activity android:name=".activity.Pembayaran" />
        <activity android:name=".activity.AddAlamat" />
        <activity android:name=".activity.ListAlamat" />
        <activity android:name=".activity.Pengiriman" />
        <activity android:name=".activity.DetailProduk" />
        <activity android:name=".activity.Riwayat" />
        <activity android:name=".activity.Baru" />
        <activity android:name=".activity.Register" />
        <activity android:name=".activity.LogReg" />
        <activity android:name=".activity.Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>